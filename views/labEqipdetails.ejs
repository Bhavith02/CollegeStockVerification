<%- include('partials/header1')%>

<div class="filter">
  <a href="/laboverview"><i class="fas fa-angle-double-left">Go Back</i></a>
</div>
<div class="lab_listing_header">
  <h1>Lab Equipments Details</h1>
</div>
<div class="newLabEquiptext">
  <a href="/addlabequip" class="border-shadow addbutton" <span class="text-gradient" role="button">Add New Equipment<i class="fas fa-user"></i></span>
  </a>
</div>

<div class="lab">
  <table class=" table-striped labtable">
    <thead class="thead-dark">
      <tr>
        <th class="th2" scope="col">ID</th>
        <th class="th2" scope="col">Item Name</th>
        <th class="th2" scope="col">Company Name</th>
        <th class="th2" scope="col">Model No</th>
        <th class="th2" scope="col">DOI</th>
        <th class="th2" scope="col">Condition</th>
        <th class="th2" scope="col">Action</th>
      </tr>
    </thead>
    <tbody>
      <%labequiplist.forEach(labequip =>{%>
      <tr>
        <th class="td2" scope="row"><%= labequip.lab_id %></th>
        <td class="td2"><%= labequip.item_name %></td>
        <td class="td2"><%= labequip.company_name %></td>
        <td class="td2"><%= labequip.model_no %></td>
        <td class="td2"><%= labequip.doi %></td>
        <td class="td2"><%= labequip.condition %></td>
        <td class="td2">
          <a href="" class="btn border-shadow update">
            <span  class="text-gradient"> <i class="fas fa-pencil-alt"></i></span>
          </a>
          <a href="" class="btn border-shadow delete">
            <span class="text-gradient"> <i class="fas fa-times"></i></span>
          </a>
        </td>
      </tr>
      <%})%>
    </tbody>
  </table>
</div>

<%- include('partials/footer1')%>
